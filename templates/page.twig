{% extends 'layouts/master.twig' %}

{% block title %}{{ item.seotitle }}{% endblock title %}

{% block content %}
    <article>
        <header>
            <h1>{{ item.title }}</h1>
            <p>Published: <time pubdate="pubdate">{{ item.date | date_default_format }}</time></p>
        </header>
        {{ item.content | raw }}
    </article>
{% endblock content %}