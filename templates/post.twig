{% extends 'layouts/master.twig' %}

{% block title %}{{ item.seoTitle }}{% endblock title %}

{% block content %}
    <header>
        <h1>{{ item.title }}</h1>
        <p>Published: <time pubdate="pubdate">{{ item.date | date_default_format }}</time></p>
    </header>
    {{ item.content | raw }}
    <footer>
        {% include 'partials/taxonomies.twig' %}
    </footer>
{% endblock content %}